{
  "routes": [
    {
      "route": "/*",
      "headers": {
        "cache-control": "public, max-age=604800, immutable"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".html": "text/html",
    ".png": "image/png",
    ".js": "application/javascript"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Content-Security-Policy": "default-src 'self' 'unsafe-inline'; img-src 'self' data:; style-src 'self' 'unsafe-inline';"
  }
}
